#include <iostream>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char** argv) {
	int vec[5]={77,9,45,30,90};
	int minimo, j, i, n, k, temp;
	for(i=0; i<n-1; i++){
		minimo=1;
		for(j=i+1; j<n; j++){
			
			if(vec[minimo] > vec[j]) minimo=j;
		}
		temp=vec[minimo];
		vec[minimo]=vec[i];
		vec[i]=temp;
	}
	for(k=0; k=4; k++){
		printf("\t%d",vec[k]);
	}
	
	
	return 0;
}
